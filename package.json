{
  "name": "frank-fo-svelte",
  "version": "5.0.0",
  "main": "dist/index.js",
  "author": "@gairal",
  "license": "MIT",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "vite build",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "deploy": "yarn build && gcloud app deploy --project com-gairal-frank-22",
    "dev": "vite",
    "gcp-build": "yarn build",
    "lint": "eslint . --ext .js,.ts,.svelte",
    "prepare": "husky install",
    "preview": "vite preview",
    "start": "sirv public --no-clear",
    "test-cov": "yarn test --collectCoverage",
    "test": "LOG_LEVEL=error jest"
  },
  "devDependencies": {
    "@sveltejs/vite-plugin-svelte": "^1.0.0-next.30",
    "@tsconfig/svelte": "^2.0.0",
    "@types/jest": "^27.4.0",
    "autoprefixer": "^10.4.1",
    "eslint": "^8.6.0",
    "eslint-config-airbnb-typescript-prettier": "^5.0.0",
    "eslint-plugin-jest": "^25.3.4",
    "eslint-plugin-no-only-tests": "^2.6.0",
    "eslint-plugin-svelte3": "^3.3.0",
    "eslint-plugin-testing-library": "^5.0.1",
    "eslint-plugin-typescript-sort-keys": "^2.1.0",
    "husky": "^7.0.4",
    "jest": "^27.4.6",
    "npm": "^8.3.0",
    "postcss": "^8.4.5",
    "prettier": "^2.5.1",
    "sass": "^1.46.0",
    "svelte": "^3.0.0",
    "svelte-check": "^2.0.0",
    "svelte-preprocess": "^4.10.1",
    "tailwindcss": "^3.0.11",
    "ts-jest": "^27.1.2",
    "tslib": "^2.0.0",
    "typescript": "^4.0.0",
    "vite": "^2.7.2"
  },
  "dependencies": {
    "sirv-cli": "^1.0.0",
    "svelte-routing": "^1.6.0"
  },
  "lint-staged": {
    "*.{js,cjs,ts,svelte}": [
      "eslint --cache --fix"
    ],
    "*.{json,scss,css,md}": [
      "prettier --write"
    ]
  }
}
